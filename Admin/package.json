    "@mui/icons-material": "^5.14.13",
    "@mui/joy": "^5.0.0-beta.10",
    "@mui/material": "^5.14.13",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
